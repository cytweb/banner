page {
	width: 750rpx;
	height: 100%;
	border: 1px solid #000;
	box-sizing: border-box;
}
.swiperClass {
  margin: 0;
  margin-top: 10px;
  height: 90%;
}
.slide-image {
  width: 100%;
  height: 90%;
  border-radius: 10px;
  position: relative;
}
image.active {
  transform: none;
  transition: all 0.2s ease-in 0s;
}  
image.quiet {
  transform: scale(0.8333333);
  transition: all 0.2s ease-in 0s;
}